{
  "name": "tinytracker-gui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "c3": "^0.7.15",
    "can-ndjson-stream": "^1.0.2",
    "d3": "^5.15.0",
    "react": "^16.12.0",
